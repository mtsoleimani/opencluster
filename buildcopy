
if [ -d libs ]; then rm -Rf libs; fi
if [ -d target ]; then rm -Rf target; fi
if [ -f opencluster.jar ]; then rm opencluster.jar; fi

mvn install

cp -fr target/opencluster.jar  ./
cp -fr target/libs  ./


