#scp -r libs root@s113:/opt/opencluster
#scp -r libs root@s114:/opt/opencluster
#scp -r libs root@s115:/opt/opencluster




scp opencluster.jar root@s111:/opt/opencluster
scp opencluster.jar root@s112:/opt/opencluster
scp opencluster.jar root@s113:/opt/opencluster
scp opencluster.jar root@s114:/opt/opencluster
scp opencluster.jar root@s115:/opt/opencluster
